 <!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
	xmlns:h="http://xmlns.jcp.org/jsf/html"
	xmlns:f="http://xmlns.jcp.org/jsf/core"
	xmlns:p="http://primefaces.org/ui"
	>
<h:head>
    <meta charset="UTF-8"/>
    <h:outputScript name="js/layout.js" library="sentinel-layout" />
    <h:outputStylesheet name="css/font-icon-layout.css" library="sentinel-layout" />
    <h:outputStylesheet name="css/sentinel-layout.css" library="sentinel-layout" />
    <h:outputStylesheet name="css/core-layout.css" library="sentinel-layout" />
    
	<title>#{message.sistematitulo }</title>
	<script language="JavaScript1.2" src="#{facesContext.externalContext.request.contextPath}/javascript/mascaras.js" type="text/javascript" charset="UTF-8" />

	
</h:head>
	
	<body style="background-image: url('#{facesContext.externalContext.request.contextPath}/images/back_madeira.png'); background-repeat: no-repeat;">
		
		<ui:include src="topbar.xhtml" />
		
		<div class="EmptyBox60"></div>
		
		<p:layout fullPage="false" style="margin-left:auto; margin-right:auto">
		
			    
			<p:layoutUnit position="center" style="background-color:#FFFFFF">

				<ui:insert name="miolo">
					<div style="padding: 5px">
						<div style="width: 100%;padding-top: 5px">
							<ui:insert name="tituloConteudo">Título</ui:insert>
						</div>
						<div style="width: 100%;padding-top: 5px">
							<ui:insert name="conteudo">Conteúdo</ui:insert>
						</div>
					</div>
				</ui:insert>
			</p:layoutUnit>
			
			<p:layoutUnit position="west"  header="Menu" resizable="false" closable="false" collapsible="true">  
				<h:form>
			        <p:menu style="width:200px">  
				        <p:submenu label="Cadastros" icon="ui-icon-disk" >
				        	<p:menuitem value="Fornecedores" action="#{layoutBean.iniciarTelaFornec()}"  icon="icon-truck" ajax="false" immediate="true"/>
				        	<p:menuitem value="Usuário" action="#{layoutBean.iniciarTelaUsuarios()}" icon="ui-icon-fish" ajax="false" immediate="true"/>
				        	<p:menuitem value="Perfil" rendered="false" icon="ui-icon-person" ajax="false" immediate="true"/>		
				        	<p:menuitem value="Peixe"  action="#{layoutBean.iniciarTelaPeixes()}" icon="ui-icon-fish" ajax="false" immediate="true"/>	
				        	<p:menuitem value="Camaras"  action="#{layoutBean.iniciarTelaCamaras()}" icon="ui-icon-fish" ajax="false" immediate="true"/>
				        	<p:menuitem value="Tipos Peixe"  action="#{layoutBean.iniciarTelaTipoPeixe()}" icon="ui-icon-fish" ajax="false" immediate="true"/>
				        	<p:menuitem value="Tamanho Peixe"  action="#{layoutBean.iniciarTelaTamanhoPeixe()}" icon="ui-icon-fish" ajax="false" immediate="true"/>					             
							<p:menuitem value="Embalagem"  action="#{layoutBean.iniciarTelaEmbalagem()}" icon="ui-icon-fish" ajax="false" immediate="true"/>				        
				        </p:submenu> 
				        <p:submenu label="Compra" icon="ui-icon-disk" >
				        	<p:menuitem value="Pagamento. de compras"  action="#{layoutBean.iniciarTelaBaixaCompras}"  icon="ui-icon-document" ajax="true" immediate="true"/>								         
				        	<p:menuitem value="Compras finalizadas" action="#{layoutBean.iniciarTelaComprasFinalizadas}"  icon="ui-icon-document" ajax="true" immediate="true"/>
				        	<p:menuitem value="Pagamentos pendentes" action="#{layoutBean.iniciarTelaPagamentosPendentes()}"  icon="ui-icon-document" ajax="true" immediate="true"/>
				        </p:submenu>
				        <p:submenu label="Estoque" >
				            <p:menuitem value="Entradas" ajax="false" action="#{layoutBean.iniciarTelaArmazenamento()}" icon="ui-icon-document"/>
				             <p:menuitem value="Retiradas" ajax="false" action="#{layoutBean.iniciarTelaRetirada()}" icon="ui-icon-document"/>
				        </p:submenu>
						<p:submenu label="Relatórios" >
							<p:menuitem value="Compras" ajax="false" action="#{layoutBean.iniciarTelaRelatoriCompras}" icon="ui-icon-document"/>
							<p:menuitem value="Histórico compra" ajax="false" action="#{layoutBean.iniciarTelaRelatorioHistoricoCompras}" icon="ui-icon-document"/>
							<p:menuitem value="Espelho de compra" ajax="false" action="#{layoutBean.iniciarTelaRelatorioCompraDetalhada}" icon="ui-icon-document"/>
							<p:menuitem value="Perda em armazenamento" ajax="false" action="#{layoutBean.iniciarTelaRelatorioPerdaArmazenamento}" icon="ui-icon-document"/>
							<p:menuitem value="Estoque" ajax="false" action="#{layoutBean.iniciarTelaRelatorioArmazenamento}" icon="ui-icon-document"/>
						</p:submenu>

					</p:menu>
				    
				    <p:confirmDialog global="true">
				       	<p:commandButton value="Não" type="button" styleClass="ui-confirmdialog-no" icon="ui-icon-close" />
						<p:commandButton value="Sim" type="button" styleClass="ui-confirmdialog-yes" icon="ui-icon-check" />
					</p:confirmDialog>
				    
			    </h:form>   
		    </p:layoutUnit>	
		</p:layout>
		
		<ui:insert name="adicionais">
		</ui:insert>
		
	</body>
</html>